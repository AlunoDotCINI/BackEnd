package br.com.school.educanet.repository;

import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;
import br.com.school.educanet.entity.Usuario;
import br.com.school.educanet.service.UserService;


public interface UserRepository extends JpaRepository<UserService,String>{
	Optional<Usuario>
	findByEmail(String UsuarioEmail);
		Optional<Usuario>
	 findBySenha(String usuarioSenha);
}
